<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  


    <servlet>
        <servlet-name>DeleteEmailListServlet</servlet-name>
        <servlet-class>email.DeleteEmailListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteEmailListServlet</servlet-name>
        <url-pattern>/deleteEmailList</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>SelectEmailListServlet</servlet-name>
        <servlet-class>email.SelectEmailListServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>SelectEmailListServlet</servlet-name>
        <url-pattern>/selectEmailList</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>UpEmailListServelet</servlet-name>
        <servlet-class>email.UpEmailListServelet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>UpEmailListServelet</servlet-name>
        <url-pattern>/upEmailList</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>AddToEmailListServlet</servlet-name>
        <servlet-class>email.AddToEmailListServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>AddToEmailListServlet</servlet-name>
        <url-pattern>/addToEmailList</url-pattern>
    </servlet-mapping>
    

    <servlet>
        <servlet-name>CartServlet</servlet-name>
        <servlet-class>cart.CartServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CartServlet</servlet-name>
        <url-pattern>/cart</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
	<welcome-file>
            index.jsp
        </welcome-file>
    </welcome-file-list>
</web-app>
